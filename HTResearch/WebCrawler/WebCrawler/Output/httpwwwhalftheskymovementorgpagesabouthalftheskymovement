V\u000a\u000a\u000a\u000a\u000a\u000a<!DOCTYPE html>\u000a\u000a<!--[if lt IE 7 ]>\u000a<html lang="en" class="no-js oldie ie6">\u000a<![endif]-->\u000a<!--[if IE 7 ]>\u000a<html lang="en" class="no-js oldie ie7">\u000a<![endif]-->\u000a<!--[if IE 8 ]>\u000a<html lang="en" class="no-js oldie ie8">\u000a<![endif]-->\u000a<!--[if IE 9 ]>\u000a<html lang="en" class="no-js ie9">\u000a<![endif]-->\u000a<!--[if (gt IE 9)|!(IE)]><!--> <html lang="en" class="no-js"> <!--<![endif]-->\u000a\u000a	<head>\u000a        <meta charset="utf-8">\u000a        \u000a        \u000a    \u000a                    \u000a        \u000a        \u000a        <meta name="description" content="{tv_meta}">\u000a        <meta property="og:description" content="{tv_meta}">\u000a        \u000a                \u000a        \u000a        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">\u000a        \u000a                <title>Half the Sky</title>\u000a        \u000a        \u000a        <link rel="shortcut icon" href="/page/-/images/static/favicon.ico">\u000a        \u000a        \u000a        \u000a        \u000a        \u000a        \u000a        \u000a        <link rel="stylesheet" href="http://www.halftheskymovement.org/?css=styles/index.css">\u000a        <link rel="stylesheet" href="/page/-/webfonts/symbolset.css">\u000a        <link rel="alternate" type="application/rss+xml" title="RSS" href="http://www.halftheskymovement.org/blog/rss">\u000a        \u000a        <script src="//ajax.googleapis.com/ajax/libs/jquery/1.7.2/jquery.min.js"></script>\u000a        \u000a        <script type="text/javascript" src="http://use.typekit.com/fut7ref.js"></script>\u000a        <script type="text/javascript">try{Typekit.load();}catch(e){}</script>\u000a        \u000a        \u000a        <!-- blue_source_tracking enabled --> \u000a\u000a        \u000a        <!--[if lt IE 9]>\u000a        <script src="//html5shiv.googlecode.com/svn/trunk/html5.js"></script>\u000a        <![endif]-->\u000a     