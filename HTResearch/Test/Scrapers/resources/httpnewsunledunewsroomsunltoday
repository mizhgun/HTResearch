ent content grid12 first">\u000a  <div id="block-system-main" class="block block-system">\u000a\u000a    \u000a  <div class="content">\u000a    <div id="newsroom-front-page">\u000a  <div id="ucomm-alert"></div>\u000a\u000a<script type="text/javascript">\u000a  WDN.loadJQuery(function() {\u000a    WDN.jQuery.get("http://www.unl.edu/ucomm/sharedcode/alert.shtml", function(data) {\u000a      WDN.jQuery("#ucomm-alert").html(WDN.jQuery.trim(data));\u000a    });\u000a  });\u000a</script><div class="view view-important-articles view-id-important_articles view-display-id-default view-dom-id-4c21fc7e84a650b4e30890810a3caeca">\u000a        \u000a  \u000a  \u000a  \u000a  \u000a  \u000a  \u000a  \u000a  \u000a</div>\u000a<div class="wdn-inner-wrapper wdn-grid-set">\u000a  <div id='left-col' class="bp2-wdn-col-four-sixths">\u000a    <div id="lead">\u000a      <div class="view view-lead-story view-id-lead_story view-display-id-default view-dom-id-41789aa8dfb367a7c2caba7d3ab63cce">\u000a        \u000a  \u000a  \u000a      <div class="view-content">\u000a        <div class="views-row views-row-1 views-row-odd views-row-first views-row-last">\u000a    <article  class="ds-1col node node-article view-mode-large_teaser clearfix">\u000a\u000a  \u000a  <div class="field field-name-field-media-collection field-type-field-collection field-label-hidden clearfix">\u000a    <div class="field-items">\u000a          <div class="field-item even"><div class="entity entity-field-collection-item field-collection-item-field-media-collection clearfix">\u000a  <div class="content">\u000a    <div class="field field-name-field-media field-type-file field-label-hidden clearfix">\u000a    <div class="field-items">\u000a          <div class="field-item even"><div  class="ds-1col file file-image file-image-jpeg view-mode-large_teaser clearfix">\u000a\u000a  \u000a  <img src="http://news.unl.edu/sites/default/files/styles/large_sixteen_by_nine/public/media/innov-campus-audit2.jpg?itok=Kcc-7KYu" width="960" 